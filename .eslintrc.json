{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": [
            "enabled",
            {
                "ignore": "ignore",
                "customValidators": "customValidator",
                "skipUndeclared": "skipUndeclared"
            }
        ]
    }
}